{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true

  },
  "rules": {
    "arrow-parens": "off",
    "linebreak-style": 0,
    "ignoreComments": 0,
    "require-jsdoc": 0,
    "operator-linebreak": "off",
    "max-len": ["error", { "code": 120 }]
  },
  
  "extends": ["eslint:recommended", "google"]
}